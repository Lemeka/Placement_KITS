import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
       int n  = sc.nextInt();
       int d , sum = 0; 
       while(n>=10)
       {
           sum = 0;
           while(n>0)
           {
               d = n%10 ;
               n = n/10;
               sum = sum + d;
           }
           n = sum;
       }
       System.out.print(n);
       
    }
}
